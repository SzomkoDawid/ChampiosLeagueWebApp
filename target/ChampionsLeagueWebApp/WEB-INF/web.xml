<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
          <servlet-name>TeamList</servlet-name>
    <servlet-class>pl.ds.servlet.TeamList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TeamList</servlet-name>
    <url-pattern>/Teams</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>PlayerList</servlet-name>
  <servlet-class>pl.ds.servlet.PlayerList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PlayerList</servlet-name>
    <url-pattern>/Best_Scorers</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CanadianRank</servlet-name>
    <servlet-class>pl.ds.servlet.CanadianRank</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CanadianRank</servlet-name>
    <url-pattern>/canadian</url-pattern>
  </servlet-mapping>
</web-app>
